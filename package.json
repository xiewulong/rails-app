{
  "name": "rails-app",
  "private": true,
  "scripts": {
    "apidoc": "rm -rf ./public/apidoc && mkdir -p ./public/apidoc && apidoc -i ./app/controllers/ -o ./public/apidoc/",
    "swagger": "rm -rf ./public/swagger && mkdir -p ./public/swagger && cp -r ./node_modules/swagger-ui-dist/* ./public/swagger/ && apidoc-swagger -i ./app/controllers/ -o ./public/swagger/ && sed -i 's/https:\\/\\/petstore.swagger.io\\/v2\\///g' ./public/swagger/index.html"
  },
  "dependencies": {
    "apidoc": "^0.25.0",
    "apidoc-swagger": "^0.3.2",
    "swagger-ui-dist": "^3.32.1"
  }
}
